form(v-on:submit.prevent="onSubmit")
    div.form-group
        label(for="notebook-title") Title
        input#notebook-title.form-control(type="text" v-model="title")
    div.form-group
        label(for="notebook-desc") Desc
        input#note-desc.form-control(type="text" v-model="description")
    button.btn.btn-default(type="submit") Submit